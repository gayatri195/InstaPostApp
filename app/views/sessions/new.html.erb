
<p id="alert"><%= alert %></p>
<h4>Login</h4>
<div class="login">
<%= form_tag sessions_path do |form| %>
  <div class="field">
    <%= label_tag :email, id="Email" %>
    <%= text_field_tag :email %>
  </div><br> 
  <div class="field">
    <%= label_tag :password, id="Password" %><br>
    <%= password_field_tag :password %>
  </div> <br><br> 
  <div class="actions">
    <%= submit_tag "Login", :id=>'loginbtn'%>
  </div>
  </div>

<% end %>
